<%- if controller_name != 'sessions' %>
  <%= link_to "Sign in", new_session_path(resource_name), class: "font-semibold leading-6 text-indigo-600 hover:text-indigo-500" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "Sign up", new_registration_path(resource_name), class: "font-semibold leading-6 text-indigo-600 hover:text-indigo-500" %><br />
<% end %>

